import random
temp=random.randint(30,100)
hum=random.randint(30,100)
if (temp>58 and hum>50):
  print("Clear the area")
elif (temp>58 or hum>50):
  if (temp<58 or hum>50):
    print("Humidity is high")
  elif(temp>58 or hum<50):
    print("Temp is high")
elif(temp<58 or hum<50):
  print("everything is fine")
